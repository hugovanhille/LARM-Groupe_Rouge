<launch>
    <param name="/use_sim_time" value="true" />
    <include file="$(find larm)/launch/challenge-1.launch" />
    <include file="$(find turtlebot_bringup)/launch/minimal.launch"/>
    <node pkg="urg_node" type="urg_node" name="laser" />
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find grp-rouge)/rviz/rosbag.rviz"/>
    <node name="gmapping" pkg="gmapping" type="slam_gmapping"/>
    <node pkg="grp-rouge" type="markbottle.py" name="mark" output="screen" />
    <node pkg="grp-rouge" name="move_turtlebot" type="move"/>
    <node pkg="grp-rouge" type="detectbottle.py" name="detection"/>
</launch>

